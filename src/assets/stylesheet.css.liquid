  line-height: 33px;
  text-align: center! important;
  width: 35px! important;
  height: 35px! important;
  padding-left: 2px;
  right:10px;
}

a.flex-prev {
  line-height: 33px;
  text-align: center! important;
  width: 35px! important;
  height: 35px! important;
  padding-right: 2px;
  left:10px;
}

.flex-direction-nav a:hover {
  background: {{ settings.directional-hover-background }};
