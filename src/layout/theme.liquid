	</head>

	<body class="gridlock shifter-left shifter {% if template contains 'product' %}product{% elsif template contains 'collection' %}collection{% else %}{{ template }}{% endif %} pg-{{page.handle}}">
		
		{% include '__opt-nprogress' %}

		<!-- Google Tag Manager (noscript) -->
		<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-N89VQM7"
		height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
		<!-- End Google Tag Manager (noscript) -->

		<div class="loader" id="__loader">
			<div>&nbsp;</div>
		</div>

		<div id="__preloader" class="opacity">
		
		{% section 'mobile-navigation' %}
		
			<div class="shifter-page{% if settings.ajax_cart_method == 'drawer' %} is-moved-by-drawer{% endif %}" id="PageContainer">
				<div id="cst-overlay"></div>
				<div class="page-wrap">
					{% section 'header' %}
					<div class="clear"></div>
					{% if template contains 'collection' %}
					{% if collection.description contains '[banner]' %}
					{% capture banner %}{{ collection.handle }}.jpg{% endcapture %}
					{% capture alt %}{% comment %}ly_code_replace_for_[ collection.title ]_begin{% endcomment %}{% include 'ly-title' with collection %}{{ ly_translation }}{% comment %}ly_code_replace_end{% endcomment %}{% endcapture %}
					<div class="collection-banner">
						<img src="{{ banner | file_url }}" alt="{% comment %}ly_code_replace_for_[ collection.title ]_begin{% endcomment %}{% include 'ly-title' with collection %}{{ ly_translation }}{% comment %}ly_code_replace_end{% endcomment %}">
					</div>
					{% endif %}
					{% endif %}


					{% if template == 'index' or template contains 'blog' or template contains 'list-collections' %}
					{{ content_for_layout }}
					{% else %}
					<div id="content" class="row">
						{{ content_for_layout }}
					</div>
					{% endif %}
				</div>  <!-- close the page-wrap class -->
				{% section 'footer' %}
			</div> <!-- close the shifter-page class -->

			<!-- <a href="#" class="scrollup"><i class="icon-chevron-up icon-2x"></i></a> -->



			<!-- Add any additional scripts below this line -->
			{% include 'back-to-the-top' %}
			<div id="zoomDivPro"></div>

		</div>

		{%- if is_admin_interface == false -%}
			{% comment %} Second part of the Kernel's scripts {% endcomment %}
			<!-- START DEFER PART OF THE SCRIPTS -->
			{% comment %} {{ js_content_for_header_defer | strip_newlines }} {% endcomment %}
			<!-- END DEFER PART OF THE SCRIPTS -->
		{%- endif -%}

		
		{% include '__opt-js-theme' %}	
		
	
	{% comment %} {{ shop.metafields.loox["global_html_body"] }} {% endcomment %}
      
