			{{ content_for_header }}	
		{%- else -%}
			{% comment %} js-content-for-header.liquid is prepare Kernel's scripts to new method {% endcomment %}
			{% include '__opt-js-content-for-header' %}

			{% comment %} First part of the Kernel's scripts {% endcomment %}
			<!-- START MAIN PART OF THE SCRIPTS -->
			{{ js_content_for_header }}
			<!-- END MAIN PART OF THE SCRIPTS -->
