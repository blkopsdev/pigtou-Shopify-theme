{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}
{% comment %}ly_asset_replace_for_[ 'ajaxify.js' | asset_url | script_tag ]_begin{% endcomment %}
{% comment %} {% assign ly_asset = 'ajaxify.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }} {% endcomment %}
{% comment %}ly_asset_replace_end{% endcomment %}
<div id="CartDrawer" class="drawer drawer--right">
  <div class="drawer__header">
    <div class="cart-img">
      <i class="fa fa-shopping-cart" aria-hidden="true"></i>
    </div>
    <div class="table-cell">
    <h3>{% comment %}ly_i18n_replace_for_[ 'cart.general.title' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.general.title' | t }}{% endcapture %}{% include 'ly-static-string' with '9405481' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h3>
    </div>
    <div class="table-cell closecell">
    <span class="drawer__close js-drawer-close">
       {% comment %} {% comment %}ly_i18n_replace_for_[ 'cart.general.close_cart' | t  ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.general.close_cart' | t  }}{% endcapture %}{% include 'ly-static-string' with '9405482' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}{% endcomment %}<i class="fas fa-times" aria-hidden="true"></i>
    </span>
    </div>
  </div>
 
  <div id="CartContainer"></div>
</div>